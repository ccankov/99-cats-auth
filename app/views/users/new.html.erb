<h1>Create an account</h1>

<form action="<%= users_url %>" method="POST">
  <%= auth_token %>
  <label>Username:
    <input type="text" name="user[user_name]">
  </label>
  <label>Password:
    <input type="password" name="user[password]">
  </label>
  <input type="submit" value="Create account">
</form>

